{
	"name": "@deutschemodelunitednations/munify-resolution-editor",
	"version": "0.1.0",
	"description": "UN Resolution Editor component for MUNify applications",
	"type": "module",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./schema": {
			"types": "./dist/schema/resolution.d.ts",
			"default": "./dist/schema/resolution.js"
		},
		"./phrases": {
			"types": "./dist/phrases/index.d.ts",
			"default": "./dist/phrases/index.js"
		},
		"./phrases/de": {
			"types": "./dist/phrases/de.d.ts",
			"default": "./dist/phrases/de.js"
		},
		"./i18n": {
			"types": "./dist/i18n/index.d.ts",
			"default": "./dist/i18n/index.js"
		},
		"./i18n/de": {
			"types": "./dist/i18n/de.d.ts",
			"default": "./dist/i18n/de.js"
		}
	},
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && bun run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "bun run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22.10.7",
		"daisyui": "^5.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.7",
		"vitest": "^3.0.4"
	},
	"dependencies": {
		"zod": "^3.24.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DeutscheModelUnitedNations/munify-resolution-editor.git"
	},
	"homepage": "https://deutschemodelunitednations.github.io/munify-resolution-editor",
	"bugs": {
		"url": "https://github.com/DeutscheModelUnitedNations/munify-resolution-editor/issues"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"mun",
		"model-united-nations",
		"resolution",
		"editor",
		"component"
	],
	"author": "Deutsche Model United Nations e.V.",
	"license": "MIT"
}
